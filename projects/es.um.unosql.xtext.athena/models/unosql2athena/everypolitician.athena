Schema everypolitician

root entity Memberships
{
	common { +_id: Identifier, legislative_period_id: String, on_behalf_of_id: String, organization_id: ref<Organizations as String>+, person_id: ref<Persons as String>+, role: String, sources: aggr<Source.1>* }
	variation 1 { area_id: ref<Areas as String>+, end_date: String, start_date: String }
	variation 2 { area_id: ref<Areas as String>+, end_date: String }
	variation 3 { area_id: ref<Areas as String>+, start_date: String }
	variation 4 { area_id: ref<Areas as String>+ }
	variation 5 { end_date: String, start_date: String }
	variation 6 { end_date: String }
	variation 7 { start_date: String }
	variation 8
}

entity Identifier
{
	common { identifier: String, scheme: String }
	variation 1
}

root entity Events
{
	common { +_id: String, classification: String, end_date: String, name: String, start_date: String }
	variation 1 { identifiers: aggr<Identifier.1>* }
	variation 2 { organization_id: ref<Organizations as String>+ }
}

root entity Persons
{
