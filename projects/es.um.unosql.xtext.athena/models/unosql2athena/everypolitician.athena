	variation 2 { contact_details: aggr<Contact_detail.1>*, death_date: String, email: String, links: aggr<Link.1>*, other_names: aggr<Other_name.2 , Other_name.1>* }
	variation 3 { contact_details: aggr<Contact_detail.1>*, email: String, links: aggr<Link.1>*, other_names: aggr<Other_name.1>* }
	variation 4 { contact_details: aggr<Contact_detail.1>*, email: String, links: aggr<Link.1>*, other_names: aggr<Other_name.2 , Other_name.1>* }
	variation 5 { contact_details: aggr<Contact_detail.1>*, email: String, links: aggr<Link.1>* }
	variation 6 { contact_details: aggr<Contact_detail.1>*, email: String }
	variation 7 { contact_details: aggr<Contact_detail.1>*, links: aggr<Link.1>*, other_names: aggr<Other_name.1>* }
	variation 8 { death_date: String, links: aggr<Link.1>*, other_names: aggr<Other_name.1>* }
	variation 9 { death_date: String, links: aggr<Link.1>*, other_names: aggr<Other_name.2 , Other_name.1>* }
	variation 10 { death_date: String }
	variation 11 { links: aggr<Link.1>*, other_names: aggr<Other_name.1>* }
	variation 12 { links: aggr<Link.1>*, other_names: aggr<Other_name.2 , Other_name.1>* }
	variation 13 { links: aggr<Link.1>* }
	variation 14
}

root entity Organizations
{
	common { +_id: String, classification: String, name: String }
	variation 1 { identifiers: aggr<Identifier.1>*, image: String, links: aggr<Link.1>*, other_names: aggr<Other_name.1>*, srgb: String }
	variation 2 { identifiers: aggr<Identifier.1>*, image: String, links: aggr<Link.1>*, other_names: aggr<Other_name.1>* }
	variation 3 { identifiers: aggr<Identifier.1>*, links: aggr<Link.1>*, other_names: aggr<Other_name.1>*, srgb: String }
	variation 4 { identifiers: aggr<Identifier.1>*, links: aggr<Link.1>*, other_names: aggr<Other_name.1>* }
	variation 5 { identifiers: aggr<Identifier.1>*, seats: Number, type: String }
	variation 6
}

entity Other_name
{
	common { name: String, note: String }
	variation 1 { lang: String }
	variation 2
}

entity Image
{
	common { url: String }
	variation 1
}

entity Contact_detail
{
	common { type: String, value: String }
	variation 1
}

entity Link
{
	common { note: String, url: String }
	variation 1
}

entity Source
{
	common { url: String }
	variation 1
}

root entity Areas
{
	common { +_id: String, name: String, type: String }
	variation 1
}
