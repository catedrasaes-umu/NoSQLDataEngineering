ScriptFeature_Ops operations

SCRIPT MODE

ADD ENTITY EntityToRemove: { +id: Identifier, attrToDel, attrToDelStr: String, attrToDelNum: Number, attrToDelDouble: Double, attrToDelBool: Boolean,
    +attrToRen, +attrToRenStr: String, +attrToRenNum: Number, +attrToRenDouble: Double, +attrToRenBool: Boolean}

DELETE EntityToRemove::attrToDel, attrToDelStr, attrToDelNum, attrToDelDouble, attrToDelBool

RENAME EntityToRemove::attrToRen TO attrTrue1
RENAME EntityToRemove::attrToRenStr TO attrTrue2
RENAME EntityToRemove::attrToRenNum TO attrTrue3
RENAME EntityToRemove::attrToRenDouble TO attrTrue4
RENAME EntityToRemove::attrToRenBool TO attrTrue5

ADD ENTITY EntityToCopySource1: { +id: String("id1"), valueToCopy1: String("COPY_THIS"), valueToCopy2: List, valueToCopy3: Map}
ADD ENTITY EntityToCopySource2: { +id: String("id2"), valueToCopy4: String("COPY_THAT")}
ADD ENTITY EntityToCopyTarget: { +id: String, thisRef: String("id1"), thatRef: String("id2") }

COPY EntityToCopySource1::valueToCopy1 TO EntityToCopyTarget::copy1 WHERE id = thisRef
COPY EntityToCopySource1::valueToCopy2 TO EntityToCopyTarget::copy2 WHERE id = thisRef
COPY EntityToCopySource1::valueToCopy3 TO EntityToCopyTarget::copy3 WHERE id = thisRef
COPY EntityToCopySource2::valueToCopy4 TO EntityToCopyTarget::valueToCopy4 WHERE id = thatRef

ADD ENTITY EntityToMoveSource1: { +id: String("id1"), valueToMove1: String("MOVE_THIS"), valueToMove2: List, valueToMove3: Map}
ADD ENTITY EntityToMoveSource2: { +id: String("id2"), valueToMove4: String("MOVE_THAT")}
ADD ENTITY EntityToMoveTarget: { +id: String, thisRef: String("id1"), thatRef: String("id2") }

MOVE EntityToMoveSource1::valueToMove1 TO EntityToMoveTarget::move1 WHERE id = thisRef
MOVE EntityToMoveSource1::valueToMove2 TO EntityToMoveTarget::move2 WHERE id = thisRef
MOVE EntityToMoveSource1::valueToMove3 TO EntityToMoveTarget::move3 WHERE id = thisRef
MOVE EntityToMoveSource2::valueToMove4 TO EntityToMoveTarget::move4 WHERE id = thatRef

ADD ENTITY EntityToNest: { +id: String, name, age, street, postalCode, city, aList: List, aMap: Map}
ADD AGGR EntityToNest::nestAggrNest1: {nest1, nest2, nest3}&
ADD AGGR EntityToNest::nestAggrNest2: {nest4, nest5, nest6}+
NEST EntityToNest::street, postalCode, city, nestAggrNest1, nestAggrNest2 TO nestedAggr

ADD ENTITY EntityToUnnest: { +id: String, name, age }
ADD AGGR EntityToUnnest::unnestAggrNest1: {nest1, nest2, nest3, street, postalCode, city, aList: List, aMap: Map}&
ADD AGGR EntityToUnnest::unnestAggrNest2: {nest4, nest5, nest6}&
UNNEST EntityToUnnest::unnestAggrNest1.street, unnestAggrNest1.postalCode, unnestAggrNest1.city, unnestAggrNest1.aList, unnestAggrNest1.aMap, unnestAggrNest2.nest4
